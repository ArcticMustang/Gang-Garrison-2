// argument0 - object (usually the calling object)
// argument1 - x position
// argument2 - y position
// argument3 - width
// argument4 - spacing

with (argument0)
{
    virtualitem = -1;
    xbegin = argument1;
    ybegin = argument2;
    width = argument3;
    spacing = argument4;
    items = 0;
    editing = -1;
    dimmed = false;
}
